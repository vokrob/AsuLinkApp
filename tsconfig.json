{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "baseUrl": "./src",
    "paths": {
      "@assets/*": [
        "../assets/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js"
  ]
}